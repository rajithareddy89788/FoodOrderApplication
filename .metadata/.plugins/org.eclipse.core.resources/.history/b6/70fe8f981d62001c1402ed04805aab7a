package com.mouritech.swiggy_application.dto;

public class RestaurentDto3 {
	private Long id;

	private String restaurentName;
	private String restaurentLocation;
	private int offerValue;

	public RestaurentDto3() {
		// TODO Auto-generated constructor stub
	}

	
	public RestaurentDto3(String restaurentName, String restaurentLocation, int offerValue) {
		super();
		this.restaurentName = restaurentName;
		this.restaurentLocation = restaurentLocation;
		this.offerValue = offerValue;
	}


	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getRestaurentName() {
		return restaurentName;
	}

	public void setRestaurentName(String restaurentName) {
		this.restaurentName = restaurentName;
	}

	public String getRestaurentLocation() {
		return restaurentLocation;
	}

	public void setRestaurentLocation(String restaurentLocation) {
		this.restaurentLocation = restaurentLocation;
	}


	public int getOfferValue() {
		return offerValue;
	}


	public void setOfferValue(int offerValue) {
		this.offerValue = offerValue;
	}

	

}
